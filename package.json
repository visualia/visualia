{
  "name": "visualia",
  "version": "0.0.10",
  "main": "./dist/visualia.umd.js",
  "module": "./dist/visualia.es.js",
  "exports": {
    ".": {
      "import": "./dist/visualia.es.js",
      "require": "./dist/visualia.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/visualia/visualia.git"
  },
  "keywords": [
    "vite",
    "vue",
    "vue-component"
  ],
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vite build && tsc src/index.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist --target esnext --module esnext --moduleResolution node",
    "release": "node scripts/release.js",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@types/animejs": "^3.1.3",
    "animejs": "^3.2.1",
    "cli-marked": "^5.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "execa": "^5.1.0",
    "katex": "^0.13.11",
    "prettier": "^2.3.0",
    "semver": "^7.3.5",
    "typescript": "^4.1.3",
    "vite": "^2.3.5",
    "vitepress": "^0.14.0",
    "vue": "^3.0.5"
  }
}
